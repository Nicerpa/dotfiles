[init]
    defaultBranch = master
[user]
    email = nicolas.cerpa@sansano.usm.cl
	name = Nicol√°s Cerpa
[core]
    editor = nvim
[pull]
    rebase = false

[includeIf "gitdir:~/Nursoft/"]
  path = ~/Nursoft/.gitconfig-nursoft

[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	func = 146 bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
